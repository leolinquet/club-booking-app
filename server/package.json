{
  "name": "sportsclubnet-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "node server.js",
    "start": "npm run migrate && node server.js",
    "start:prod": "npm run migrate && node server.js",
    "migrate": "node migrate.mjs",
    "migrate:safe": "node dev-safety.mjs pre-migration && node migrate.mjs",
    "migrate:check": "node db-sync.mjs --check-only",
    "migrate:production": "node db-sync.mjs --production",
    "migrate:check:prod": "node db-sync.mjs --production --check-only",
    "migrate:force": "ALLOW_DESTRUCTIVE_MIGRATIONS=1 node migrate.mjs",
    "migrate:new": "node create_migration.mjs",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "db:sync": "node db-sync.mjs",
    "db:sync:prod": "node db-sync.mjs --production",
    "db:backup": "node db-backup.mjs create",
    "db:backup:list": "node db-backup.mjs list",
    "db:backup:clean": "node db-backup.mjs clean",
    "db:backup:restore": "node db-backup.mjs restore",
    "db:backup:daily": "node dev-safety.mjs daily",
    "db:health": "node db-health.mjs",
    "db:safety": "node dev-safety.mjs safety",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "studio": "prisma studio",
    "check:db": "node check_db.mjs",
    "check:seed": "node check_seed.mjs"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.3.0",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "resend": "^6.0.3"
  },
  "devDependencies": {
    "prisma": "^6.0.1"
  }
}
